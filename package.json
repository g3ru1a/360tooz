{
    "name": "spintooz",
    "version": "1.0.0",
    "description": "Convert youtooz product page into 360 spin GIF",
    "main": "src/index.js",
    "bin": {
        "@g3ru1a/spintooz": "bin/spintooz",
        "spintooz": "bin/spintooz"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "cli",
        "scaffolding"
    ],
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "dev": "ts-node-dev ./src/index.ts",
        "serve": "node ./dist/index.js",
        "start": "npm run build && npm run serve"
    },
    "files": [
        "bin/",
        "src/",
        "templates/"
    ],
    "author": "G3ru1a",
    "license": "MIT",
    "dependencies": {
        "arg": "^5.0.0",
        "aws-sdk": "^2.804.0",
        "axios": "^0.21.0",
        "chalk": "^4.1.0",
        "cheerio": "^1.0.0-rc.3",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "gifencoder": "^2.0.1",
        "image-size": "^0.9.3",
        "inquirer": "^7.3.3",
        "typescript": "^4.0.5"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.22",
        "@types/cors": "^2.8.8",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.9",
        "@types/gifencoder": "^2.0.0",
        "@types/inquirer": "^7.3.1",
        "nodemon": "^2.0.6",
        "ts-node": "^9.0.0",
        "ts-node-dev": "^1.0.0"
    }
}
